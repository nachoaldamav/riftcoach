{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "strict": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@riftcoach/packages.shared.aggregations": [
        "../../packages/shared/aggregations/dist/index.d.ts",
        "../../packages/shared/aggregations/dist/index.js"
      ]
    },
    "types": [
      "node",
      "react"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "outDir": "./dist"
  },
  "exclude": ["node_modules"]
}
